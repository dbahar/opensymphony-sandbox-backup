<?xml version="1.0" encoding="ISO-8859-1"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"         version="2.4">    <display-name>Able</display-name>    <context-param>        <param-name>contextConfigLocation</param-name>        <param-value>classpath:applicationContext.xml</param-value>    </context-param>    <filter>        <filter-name>i18n</filter-name>        <filter-class>com.opensymphony.able.util.I18nFilter</filter-class>    </filter>    <filter>        <display-name>Stripes Filter</display-name>        <filter-name>StripesFilter</filter-name>        <filter-class>net.sourceforge.stripes.controller.StripesFilter</filter-class>        <init-param>            <param-name>ActionResolver.Class</param-name>            <param-value>com.opensymphony.able.stripes.util.AbleNameBasedActionResolver</param-value>        </init-param>        <init-param>            <param-name>ActionResolver.UrlFilters</param-name>            <param-value>/classes/</param-value>        </init-param>        <init-param>            <param-name>ActionBeanContext.Class</param-name>            <param-value>com.opensymphony.able.demo.action.util.AbleActionBeanContext</param-value>        </init-param>        <init-param>            <param-name>TypeConverterFactory.Class</param-name>            <param-value>com.opensymphony.able.demo.action.util.AbleTypeConverterFactory</param-value>        </init-param>        <init-param>            <param-name>FormatterFactory.Class</param-name>            <param-value>com.opensymphony.able.stripes.util.JpaFormatterFactory</param-value>        </init-param>        <init-param>            <param-name>Interceptor.Classes</param-name>            <param-value>                net.sourceforge.stripes.integration.spring.SpringInterceptor,                com.opensymphony.able.stripes.util.PartialInterceptor,              com.opensymphony.able.demo.action.util.SecurityInterceptor,              net.sourceforge.stripes.controller.BeforeAfterMethodInterceptor            </param-value>        </init-param>    </filter>    <filter>        <filter-name>sitemesh</filter-name>        <filter-class>com.opensymphony.module.sitemesh.filter.PageFilter</filter-class>    </filter>    <filter>        <filter-name>jpaTransaction</filter-name>        <filter-class>com.opensymphony.able.filter.SimpleTransactionServletFilter</filter-class>    </filter>    <filter>        <filter-name>StripesActionFilter</filter-name>        <filter-class>com.opensymphony.able.stripes.util.AbleActionBeanFilter</filter-class>    </filter>    <filter-mapping>        <filter-name>i18n</filter-name>        <url-pattern>/*</url-pattern>    </filter-mapping>    <filter-mapping>        <filter-name>StripesFilter</filter-name>        <url-pattern>/*</url-pattern>        <dispatcher>REQUEST</dispatcher>    </filter-mapping>    <filter-mapping>        <filter-name>sitemesh</filter-name>        <url-pattern>/*</url-pattern>        <dispatcher>REQUEST</dispatcher>    </filter-mapping>    <filter-mapping>        <filter-name>jpaTransaction</filter-name>        <url-pattern>/*</url-pattern>    </filter-mapping>    <filter-mapping>        <filter-name>StripesActionFilter</filter-name>        <url-pattern>/*</url-pattern>        <dispatcher>REQUEST</dispatcher>    </filter-mapping>    <listener>        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>    </listener>    <servlet>        <servlet-name>ServletHelper</servlet-name>        <servlet-class>com.opensymphony.able.stripes.util.AbleServletHelper</servlet-class>        <load-on-startup>1</load-on-startup>    </servlet>    <jsp-config>        <taglib>            <taglib-uri>/able-functions</taglib-uri>            <taglib-location>/WEB-INF/able-functions.tld</taglib-location>        </taglib>    </jsp-config></web-app>